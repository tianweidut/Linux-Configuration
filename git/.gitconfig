[user]
	name = tianweidut
	email = liutianweidlut@qq.com
[http]
	proxy = 127.0.0.1:8087
	postBuffer = 524288000
	sslVerify = false
[color]
    ui = true
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = true
    pager = true

[color "branch"]
	current = yellow reverse
  	local = yellow
  	remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = cyan
 
[core]
    editor="vim"
    autocrlf = input
    safecrlf = warn
    excludesfile = /Users/mark/.gitignore_global
    quotepath = false
    
[mergetool "sublime"]
    cmd = subl -n $MERGED
    trustExitCode = false